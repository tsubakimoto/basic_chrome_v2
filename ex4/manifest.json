{
	"name": "My fourth extension",
	"version": "0.1",
	"manifest_version": 2,
	
	"description": "今いるページの背景色を変えるよ！ with option page",
	
	"permissions": [
		"tabs", "http://*/*"
	],
	
	"options_page": "options.html",
	
	"background": {
		"scripts": ["background.js"]
	},
	
	"browser_action": {
		"default_icon": "icon1.png",
		"default_title": "My fourth extension"
	}
}